[user]
	name = Mabweh Kwasen
	email = mabweh.kwasen@myboost.co
[init]
	defaultBranch = master
[core]
	editor = vim
[color]
	ui = auto
[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold
[alias]
	br = branch --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) - %(contents:subject) %(color:green)(%(committerdate:relative)) [%(authorname)]' --sort=-committerdate
	ci = commit
    co = checkout
	st = status
	unstage = reset HEAD --
	undo = reset --soft HEAD~1
	ac = !git add -A && git commit
	contrib = shortlog -e --summary --numbered
	last = log -1 HEAD
	ll = log --graph --pretty=format:'%C(yellow)%h%C(auto)%d%Creset %s %C(white)- %an, %ar%Creset'
[safe]
	directory = C:/Users/MabwehKwasen/WORK/
	directory = C:/Users/MabwehKwasen/WORK/PROJECTS/partner-service
	directory = C:/Users/MabwehKwasen/WORK/PROJECTS/cs-portal
	directory = C:/Users/MabwehKwasen/WORK/tmp/test-git
